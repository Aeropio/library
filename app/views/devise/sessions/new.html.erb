<h1 class="H1">Admin Login </h1>

<div class="Login-Form">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

<div class="container-fluid">
<div class="row Margin-none">
<div class="col-lg-12 Margin-Bottom-10 Form-Text">User Name</div>
<div class="col-lg-12 Margin-Bottom-20">
<div class="text-box-shape">
 <%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder: "User name", class: "Text-box-login"%>
<a href="#" class="Text-box-login-a"><img src="img/User.png" width="32" height="32" alt=""></a>
</div>
</div>

<div class="col-lg-12 Margin-Bottom-10 Form-Text">Password</div>
<div class="col-lg-12 Margin-Bottom-20">
<div class="text-box-shape ">
<%= f.password_field :password, autocomplete: "current-password" %>
<a href="#" class="Text-box-login-a"><img src="img/Lock.png" width="32" height="32" alt=""></a>
</div>
</div>

<div class="col-lg-12 Margin-Bottom-20 Form-Text" ><input name="" type="checkbox" value="">&nbsp;&nbsp;Remember Me</div>

<div class="col-lg-12 Margin-Bottom-20" ><%= f.submit "Log in" %></div>

</div>
</div>

<% end %>
</div>

<%= render "devise/shared/links" %>
